{
  "name": "school-vr",
  "productName": "School VR",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "aframe": "^0.9.2",
    "aframe-extras": "^6.0.0",
    "aframe-gif-shader": "^0.2.0",
    "asar": "^0.14.6",
    "deepmerge": "^2.2.1",
    "easyrtc": "^1.1.0",
    "electron-is-dev": "^1.0.1",
    "express": "^4.17.1",
    "gsap": "^2.0.2",
    "jsonc-parser": "^2.1.0",
    "jsonschema": "^1.2.4",
    "moment": "^2.24.0",
    "mousetrap": "^1.6.3",
    "networked-aframe": "^0.6.1",
    "opentype.js": "^1.1.0",
    "p-iteration": "^1.1.7",
    "path": "^0.12.7",
    "query-string": "^6.4.2",
    "react": "^16.5.2",
    "react-color": "^2.17.3",
    "react-dom": "^16.5.2",
    "react-draggable": "^3.0.5",
    "react-loadable": "^5.5.0",
    "react-rnd": "^9.0.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "rimraf": "^2.6.3",
    "save-dev": "^2.0.0",
    "serve-static": "^1.14.1",
    "smalltalk": "^3.2.2",
    "socket.io": "^2.2.0",
    "typescript": "^3.5.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.4",
    "electron-builder": "^20.40.2",
    "wait-on": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:1234 && electron .\"",
    "electron-build": "electron-builder -c.extraMetadata.main=build/electron.js",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "react-scripts build",
    "dist-all": "electron-builder -mw"
  },
  "build": {
    "appId": "com.ioiocreative.school-vr",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "fileAssociations": {
      "ext": "ivr",
      "name": "School-VR Project"
    },
    "mac": {
      "category": "public.app-category.education"
    }
  },
  "author": "ioio-creative",
  "main": "public/electron.js",
  "homepage": "./"
}
